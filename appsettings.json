{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.AwsCloudWatch" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "CloudWatch" // Options: Console, File, Database, CloudWatch
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\myapp.log",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "AWSSeriLog",
        "Args": {
          "logGroup": "MyLogGroup",
          "logStreamNamePrefix": "MyApp-",
          "region": "us-east-1",
          "minimumLogEventLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=appdata.db"
  },
  "Jwt": {
    "Key": "[key]",
    "Issuer": "[issuer]",
    "Audience": "APIUserTokenFrontend"
  },
  "AWS": {
    "Region": "us-east-1",
    "AccessKey": "[AccessKey]",
    "SecretKey": "[SecretKey]"
  },
  "Email": {
    "From": "[yourInfoHere]@gmail.com",
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": "465",
    "Username": "[yourInfoHere]@gmail.com",
    "Password": "[yourInfoHere]"
  }
}

